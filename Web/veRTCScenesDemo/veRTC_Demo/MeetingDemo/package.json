    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
