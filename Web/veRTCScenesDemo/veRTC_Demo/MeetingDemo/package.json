    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "socket.io-client": "2.3.1",
    "umi": "^3.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.6",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.debounce": "^4.0.6",
